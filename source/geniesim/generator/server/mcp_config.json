{
  "mcpServers": {
    "assets-agent": {
      "args": [
        "generator/server/mcp_assets_server.py"
      ],
      "command": "python",
      "cwd": "${workspaceFolder}/openwebui_api_server",
      "env": {
        "PYTHONPATH": "/"
      }
    },
    "file-agent": {
      "args": [
        "generator/server/mcp_file_server.py"
      ],
      "command": "python",
      "cwd": "${workspaceFolder}/openwebui_api_server",
      "env": {
        "PYTHONPATH": "/"
      }
    }
  }
}
